{
  "name": "{{npmName}}",
  "version": "{{npmVersion}}",
  "description": "Deep3 Labs Node.js API client",
  "author": "Deep3 Labs",
  "repository": {
    "type": "git",
    "url": "https://www.github.com/deep3labs/deep3-node-client.git"
  },
  "keywords": [
    "deep3labs",
    "web3",
    "blockchain",
    "ml",
    "ai"
  ],
{{#packageAsSourceOnlyLibrary}}
  "main": "./index.ts",
{{/packageAsSourceOnlyLibrary}}
{{^packageAsSourceOnlyLibrary}}
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
{{#supportsES6}}
  "module": "./dist/esm/index.js",
  "sideEffects": false,
{{/supportsES6}}
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "generate:docs": "npx typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts && rm docs/README.md",
    "build": "npm run clean && tsc{{#supportsES6}} && tsc -p tsconfig.esm.json{{/supportsES6}}",
    "prepublishOnly": "npm run build"
  },
{{/packageAsSourceOnlyLibrary}}
  "devDependencies": {
{{#sagasAndRecords}}
    "immutable": "^4.0.0-rc.12",
    "normalizr": "^3.6.1",
    "redux-saga": "^1.1.3",
    "redux-ts-simple": "^3.2.0",
    "reselect": "^4.0.0",
{{/sagasAndRecords}}
    "rimraf": "^5.0.1",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "^4.0"
  }{{#npmRepository}},{{/npmRepository}}
{{#npmRepository}}
  "publishConfig": {
    "registry": "{{npmRepository}}"
  }
{{/npmRepository}}
}
